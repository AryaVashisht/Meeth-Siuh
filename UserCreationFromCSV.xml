          <script>var count = 0
var report = &apos;&apos;
for each(seg in msg.children()) {
    var errMessage = &quot;&quot;;
    var status = &quot;&quot;;
    if (count == 0) {
        report = report + (SerializerFactory.getSerializer(&apos;DELIMITED&apos;).fromXML(seg)).replace(&apos;\n&apos;, &apos;&apos;) + &quot;,Status,Filter Reason\n&quot;;
        logger.info(&quot;Header information &quot; + (SerializerFactory.getSerializer(&apos;DELIMITED&apos;).fromXML(seg)).replace(&apos;\n&apos;, &apos;&apos;));
    } else {
        try {
            logger.info(&quot;Inside Function to get Data&quot; + seg)
            var row = {}
            row.names = convertCSVPhysName1(seg[&apos;Name&apos;].toString())
            row.firstName = row.names.get(&apos;firstName&apos;) ? row.names.get(&apos;firstName&apos;).trim() : &quot;&quot;;
            row.lastName = row.names.get(&apos;lastName&apos;) ? row.names.get(&apos;lastName&apos;).trim() : &quot;&quot;;
            row.email = seg[&apos;Email&apos;] ? seg[&apos;Email&apos;].toString().trim().toLowerCase() : &quot;&quot;;
            row.phone = seg[&apos;Phone&apos;] ? seg[&apos;Phone&apos;].toString().trim() : &quot;&quot;;
            row.userGroupNameList = seg[&apos;userGroupName&apos;] ? seg[&apos;userGroupName&apos;].toString().trim().split(configurationMap.get(&apos;locationDelimiter&apos;)) : [];
            row.userGroupNameList = trimListValue(row.userGroupNameList)
            logger.info(&quot;userGroupName: &quot; + row.userGroupName)
            row.dob = seg[&apos;Dob&apos;] ? seg[&apos;Dob&apos;].toString().trim() : &quot;&quot;;
            row.dept = seg[&apos;Dept&apos;] ? seg[&apos;Dept&apos;].toString().trim() : &quot;&quot;
            row.gender = seg[&apos;Gender&apos;] ? seg[&apos;Gender&apos;].toString().trim() : &quot;&quot;;
            row.locationNameList = seg[&apos;locationName&apos;].toString() ? seg[&apos;locationName&apos;].toString().trim().split(configurationMap.get(&apos;locationDelimiter&apos;)) : [];
            row.locationNameList = trimListValue(row.locationNameList)
            row.locationIdentifierList = seg[&apos;locationIdentifier&apos;].toString() ? seg[&apos;locationIdentifier&apos;].toString().trim().split(configurationMap.get(&apos;locationDelimiter&apos;)) : [];
            row.locationIdentifierList = trimListValue(row.locationIdentifierList)
            row.userGroupType = seg[&apos;userGroupType&apos;] ? seg[&apos;userGroupType&apos;].toString().trim() : &quot;&quot;
            row.userType = seg[&apos;userType&apos;] ? seg[&apos;userType&apos;].toString().trim().toUpperCase() : &quot;&quot;
            row.resourceidentifier = seg[&apos;resourceidentifier&apos;] ? seg[&apos;resourceidentifier&apos;].toString().trim() : &quot;&quot;
            row.countryCode = seg[&apos;CountryCode&apos;] ? seg[&apos;CountryCode&apos;].toString().trim() : configurationMap.get(&apos;defaultCountryCode&apos;).toString()
            row.countryCode = getCountryCodePrefix(new java.lang.String(row.countryCode))
                   
            row.prefix = seg[&apos;prefix&apos;].trim() ? seg[&apos;prefix&apos;].trim().toString().trim() : &quot;&quot;
            row.countryCodeFax = seg[&apos;countryCodeFax&apos;] ? seg[&apos;countryCodeFax&apos;].toString().trim() : &quot;&quot;
            row.countryCodeFax = getCountryCodePrefix(new java.lang.String(row.countryCodeFax))
            row.faxNo = seg[&apos;faxNo&apos;] ? seg[&apos;faxNo&apos;].toString().trim() : &quot;&quot;
            row.username = seg[&apos;username&apos;] ? seg[&apos;username&apos;].toString().trim() : &quot;&quot;
            row.facilityAddressList = seg[&apos;facilityAddress&apos;] ? seg[&apos;facilityAddress&apos;].toString().trim().split(configurationMap.get(&apos;locationDelimiter&apos;)) : [];
            row.facilityAddressList = trimListValue(row.facilityAddressList)
            row.facilityCityList = seg[&apos;facilityCity&apos;] ? seg[&apos;facilityCity&apos;].toString().trim().split(configurationMap.get(&apos;locationDelimiter&apos;)) : [];
            row.facilityCityList = trimListValue(row.facilityCityList)
            row.facilityStateList = seg[&apos;facilityState&apos;] ? seg[&apos;facilityState&apos;].toString().trim().split(configurationMap.get(&apos;locationDelimiter&apos;)) : [];
            row.facilityStateList = trimListValue(row.facilityStateList)
            row.facilityPostalCodeList = seg[&apos;facilityPostalCode&apos;] ? seg[&apos;facilityPostalCode&apos;].toString().trim().split(configurationMap.get(&apos;locationDelimiter&apos;)) : [];
            row.facilityPostalCodeList = trimListValue(row.facilityPostalCodeList)
            logger.info(&quot;facilityPostalCodeList : &quot;+row.facilityPostalCodeList)

            logger.info(&quot;Going to iterate each location and usergroup for row : &quot;+row);

            var locLeng = row.locationIdentifierList.length

            if(locLeng != row.facilityAddressList.length ||locLeng != row.facilityCityList.length ||locLeng != row.facilityStateList.length ||locLeng != row.facilityPostalCodeList.length ){
                //status = &quot;FAILED&quot;;
                logger.info(&quot;LOCATION_ADDRESS_COUNT_MISMATCH&quot;);
            }
            if(locLeng ==0){
                status = &quot;FAILED&quot;;
                errMessage = &quot;LOCATION_IDENTIFIER_FIELD_IS_EMPTY&quot;;
            }
            else if(locLeng != row.userGroupNameList.length || (row.locationNameList.length &gt;0 &amp;&amp; locLeng != row.locationNameList.length)){
                status = &quot;FAILED&quot;;
                errMessage = &quot;UESRGROUP_NAME, LOCATION_NAME, LOCATION_ID COUNT_MISMATCH&quot;;
            }
            else{
                logger.info(&quot;Calling createUserViaCSV for User creation.&quot;)
                for(index = 0; (index &lt; locLeng &amp;&amp; status!=&quot;FAILED&quot;); index++){

                    row.userGroupName = row.userGroupNameList[index]
                    row.locationName = (row.locationNameList.length &gt; index) ? row.locationNameList[index] : &quot;&quot;;
                    row.locationIdentifier = row.locationIdentifierList[index]
                    row.facilityAddress = (row.facilityAddressList.length &gt; index) ? row.facilityAddressList[index] : &quot;&quot;
                    row.facilityCity = (row.facilityCityList.length &gt; index) ? row.facilityCityList[index] : &quot;&quot;;
                    row.facilityState = (row.facilityStateList.length &gt; index) ? row.facilityStateList[index] : &quot;&quot;;
                    row.facilityPostalCode = (row.facilityPostalCodeList.length &gt; index) ? row.facilityPostalCodeList[index] : &quot;&quot;;

                    logger.info(&quot;Calling createUserViaCSV for index : &quot;+index+&quot;, userGroupName : &quot;+row.userGroupName+&quot;, locationName : &quot;+row.locationName+&quot;, locationIdentifier : &quot;+row.locationIdentifier)
                    var returnMap = createUserViaCSV(row,status,errMessage);
                    status = returnMap.status;
                    errMessage = returnMap.errMessage;
                }
            }
            logger.info(&quot;ROW_PROCESS_COMPLETION : STATUS : &quot;+status+&quot;, errMessage : &quot;+errMessage)

        }
        catch(e){
            logger.error(&quot;EXCEPTION : &quot;+e);
            status = &quot;FAILED&quot;;
            errMessage = e;
        }
        report = report + (SerializerFactory.getSerializer(&apos;DELIMITED&apos;).fromXML(seg)).replace(&apos;\n&apos;, &apos;&apos;) + &quot;,&quot; + status + &quot;,&quot; + errMessage + &quot;\n&quot;;
    }
    count++;
}

logger.info(&quot;Going to add report file&quot; + report);
channelMap.put(&apos;report&apos;, report);
var date = new Date().toISOString().slice(0, 16)
var userAndUserGroupCsvToSend = &quot;userAndUserGroupFilterReport&quot;+date+&quot;.csv&quot;
channelMap.put(&apos;userAndUserGroupCsvToSend&apos;,userAndUserGroupCsvToSend)</script>
